{
  "name": "p5js-ts-starter",
  "version": "1.0.0",
  "description": "## Install",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --watch",
    "clean": "rm -rf ./build/* && cp ./src/index.html ./build/index.html && cp ./node_modules/p5/lib/p5.min.js ./build/p5.min.js",
    "build-clean": "npm run clean && tsc --build ./tsconfig.json",
    "build-serve": "npm run build-clean && serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xektrum/p5js-ts-starter.git"
  },
  "keywords": [
    "p5.js",
    "p5js",
    "typescript"
  ],
  "author": "xektrum",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xektrum/p5js-ts-starter/issues"
  },
  "homepage": "https://github.com/xektrum/p5js-ts-starter#readme",
  "devDependencies": {
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/p5": "^1.5.0",
    "p5": "^1.5.0"
  }
}
